{
    "name": "telegram-restricted-downloader",
    "displayName": "Telegram Restrcted Content Downloader",
    "version": "1.1.0",
    "description": "A restricted content downloader for telegram",
    "author": "YZH",
    "scripts": {
        "dev": "plasmo dev",
        "build": "plasmo build",
        "package": "plasmo package"
    },
    "dependencies": {
        "plasmo": "0.84.2",
        "react": "18.2.0",
        "react-dom": "18.2.0",
        "tailwindcss": "3.4.1"
    },
    "devDependencies": {
        "@ianvs/prettier-plugin-sort-imports": "4.1.1",
        "@types/chrome": "0.0.258",
        "@types/node": "20.11.5",
        "@types/react": "18.2.48",
        "@types/react-dom": "18.2.18",
        "postcss": "8.4.33",
        "prettier": "3.2.4",
        "typescript": "5.3.3"
    },
    "manifest": {
        "host_permissions": [
            "https://web.telegram.org/*"
        ],
        "permissions": [
            "scripting"
        ]
    }
}